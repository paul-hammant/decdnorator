<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>App demonstrating the use of art</display-name>

  <filter>
    <filter-name>art</filter-name>
    <filter-class>group.art.example.ExampleFilter</filter-class>
    <init-param>
      <param-name>pages</param-name>
      <param-value>one,two</param-value>
    </init-param>
  </filter>

  <filter-mapping>
    <filter-name>art</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>

</web-app>
